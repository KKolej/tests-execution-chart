<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Parallel" verbose="1" parallel="methods" thread-count="4">
    <test name="Parallel">
        <groups>
            <run>
                <include name="parallel"/>
            </run>
        </groups>
        <packages>
            <package name=".*"/>
        </packages>
    </test>
</suite>
